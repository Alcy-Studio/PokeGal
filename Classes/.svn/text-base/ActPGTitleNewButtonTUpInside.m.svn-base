#import "ActPGTitleNewButtonTUpInside.h"

//Categories
#import "ActPGTitleNewButtonTUpInside+Combo.h"

//Constants
#import "ConstantsAuditorUserDefaults.h"

@implementation ActPGTitleNewButtonTUpInside

-(id)initWithRole:(unsigned int)r{
	self = [super init];
	if(!self)
		return self;
	self.role = r;
	return self;
}
-(void)setCombo{
	switch (self.role) {
		case kAudUDNumPGTitleNew:
			[self setComboNewButton];
			break;
		default:
			break;
	}
	
};
@end

