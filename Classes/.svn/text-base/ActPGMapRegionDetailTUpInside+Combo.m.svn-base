-(void)setComboRegionNoneDetail{
	NSLog(@"ActPGMapRegionDetailTUpInside - SetComboRegionNoneDetail");
	
	combo = [[NSArray alloc]initWithObjects:nil];
}
-(void)setComboRegionADetail{
	NSLog(@"ActPGMapRegionDetailTUpInside - SetComboRegionADetail");
	
	//combo = [[NSArray alloc]initWithObjects:nil];
	
	NSString* action1 = NSStringFromSelector(@selector(modifyDirectRoleRegionA));

	NSString* action2 = NSStringFromSelector(@selector(switchViewToPGDetailWithTCurlUp));
	id target = self;
	
	NSDictionary* ta = [[NSDictionary alloc]initWithObjectsAndKeys:
						target, action1,
						target, action2,

						nil];
	
	combo = [[NSDictionary alloc]initWithDictionary:ta];
	[ta release];
	
}
