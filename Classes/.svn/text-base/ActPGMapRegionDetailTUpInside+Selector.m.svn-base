 kDetailEffect3 = 8,		//View8, Diary List
 
 kDetailDescription = 9,		//View9, Diary List
 */

-(void)modifyDirectRoleRegionA{
	
	//Change role of Picture, Top Lv, Mid Lv, Low Lv, DetailButton 
	[MUi modifyTag:kDetailThumb role:kAudUDNumPGDetailThumbA scene:kSceneCodePGDetail];
	[MUi modifyTag:kDetailTitle role:kAudUDNumPGDetailRegionATitle scene:kSceneCodePGDetail];
	[MUi modifyTag:kDetailEffect1 role:kAudUDNumPGDetailRegionAEffect1 scene:kSceneCodePGDetail];
	[MUi modifyTag:kDetailEffect2 role:kAudUDNumPGDetailRegionAEffect2 scene:kSceneCodePGDetail];
	[MUi modifyTag:kDetailEffect3 role:kAudUDNumPGDetailRegionAEffect3 scene:kSceneCodePGDetail];
	[MUi modifyTag:kDetailDescription role:kAudUDNumPGDetailRegionADescription scene:kSceneCodePGDetail];

	
}
 
