#import "ActPGRomanceMsgButtonTUpInside.h"

//Categories
#import "ActPGRomanceMsgButtonTUpInside+Combo.h"

//Constants
#import "ConstantsAuditorUserDefaults.h"

@implementation ActPGRomanceMsgButtonTUpInside

-(id)initWithRole:(unsigned int)r{
	self = [super init];
	if(!self)
		return self;
	self.role = r;
	return self;
}
-(void)setCombo{
	switch (self.role) {
		case kAudUDNumPGRomanceMsgButton:
			[self setComboMsgButton];
			break;
		default:
			break;
	}
	
};
@end

