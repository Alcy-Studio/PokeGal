#import "ActPGMConfirmRightButtonTUpInside.h"

//Categories
#import "ActPGMConfirmRightButtonTUpInside+Combo.h"

//Constants
#import "ConstantsAuditorUserDefaults.h"

@implementation ActPGMConfirmRightButtonTUpInside

-(id)initWithRole:(unsigned int)r{
	self = [super init];
	if(!self)
		return self;
	self.role = r;
	return self;
}
-(void)setCombo{
	switch (self.role) {
		case kAudUDNumPGMConfirmRightButton:
			[self setComboRightButton];
			break;
		default:
			break;
	}
	
};

@end
