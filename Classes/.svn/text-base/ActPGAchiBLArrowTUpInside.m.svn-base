#import "ActPGAchiBLArrowTUpInside.h"

//Categories
#import "ActPGAchiBLArrowTUpInside+Combo.h"

//Constants
#import "ConstantsAuditorUserDefaults.h"

@implementation ActPGAchiBLArrowTUpInside

-(id)initWithRole:(unsigned int)r{
	self = [super init];
	if(!self)
		return self;
	self.role = r;
	return self;
}
-(void)setCombo{
	switch (self.role) {
		case kAudUDNumPGMainLArrow:
			[self setComboLArrow];
			break;
		default:
			break;
	}
	
};

@end

