#import "ActPGTitleContinueButtonTUpInside.h"

//Categories
#import "ActPGTitleContinueButtonTUpInside+Combo.h"

//Constants
#import "ConstantsAuditorUserDefaults.h"

@implementation ActPGTitleContinueButtonTUpInside

-(id)initWithRole:(unsigned int)r{
	self = [super init];
	if(!self)
		return self;
	self.role = r;
	return self;
}
-(void)setCombo{
	switch (self.role) {
		case kAudUDNumPGTitleContinue:
			[self setComboContinueButton];
			break;
		default:
			break;
	}
	
};
@end

