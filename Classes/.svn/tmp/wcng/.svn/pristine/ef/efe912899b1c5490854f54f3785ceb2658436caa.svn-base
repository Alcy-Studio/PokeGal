#import "ActPGDateRActButtonTUpInside.h"

//Categories
#import "ActPGDateRActButtonTUpInside+Combo.h"

//Constants
#import "ConstantsAuditorUserDefaults.h"

@implementation ActPGDateRActButtonTUpInside

-(id)initWithRole:(unsigned int)r{
	self = [super init];
	if(!self)
		return self;
	self.role = r;
	return self;
}
-(void)setCombo{
	switch (self.role) {
		case kAudUDNumPGDateRActButton:
			[self setComboActLeave];
			break;
		default:
			break;
	}
	
};
@end

