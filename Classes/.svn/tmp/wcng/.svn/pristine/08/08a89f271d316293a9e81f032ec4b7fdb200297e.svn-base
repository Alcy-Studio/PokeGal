#import "ActPGDateLActButtonTUpInside.h"

//Categories
#import "ActPGDateLActButtonTUpInside+Combo.h"

//Constants
#import "ConstantsAuditorUserDefaults.h"

@implementation ActPGDateLActButtonTUpInside

-(id)initWithRole:(unsigned int)r{
	self = [super init];
	if(!self)
		return self;
	self.role = r;
	return self;
}
-(void)setCombo{
	switch (self.role) {
		case kAudUDNumPGDateLActButton:
			[self setComboLActButton];
			break;
		default:
			break;
	}
	
};
@end

