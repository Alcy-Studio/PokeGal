#import "ActPGMapNavButtonGTUpInside.h"

//Categories
#import "ActPGMapNavButtonGTUpInside+Combo.h"

//Constants
#import "ConstantsAuditorUserDefaults.h"

@implementation ActPGMapNavButtonGTUpInside

-(id)initWithRole:(unsigned int)r{
	self = [super init];
	if(!self)
		return self;
	self.role = r;
	return self;
}
-(void)setCombo{
	switch (self.role) {
		case kAudUDNumPGMapRegionG:
			[self setComboNavButtonG];
			break;
		default:
			break;
	}
	
};
@end

