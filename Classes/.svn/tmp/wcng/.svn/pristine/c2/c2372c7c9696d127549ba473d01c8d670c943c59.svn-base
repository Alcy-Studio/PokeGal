#import "ActPGMainRArrowTUpInside.h"

//Categories
#import "ActPGMainRArrowTUpInside+Combo.h"

//Constants
#import "ConstantsAuditorUserDefaults.h"

@implementation ActPGMainRArrowTUpInside

-(id)initWithRole:(unsigned int)r{
	self = [super init];
	if(!self)
		return self;
	self.role = r;
	return self;
}
-(void)setCombo{
	switch (self.role) {
		case kAudUDNumPGMainRArrow:
			[self setComboRArrow];
			break;
		default:
			break;
	}
	
};

@end
